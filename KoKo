class Solution {
    public int minEatingSpeed(int[] piles, int h) {
        int l =1;
        int hi = Integer.MAX_VALUE;
        while(hi>=l){
        int mid = l+(hi-l)/2;
        int s =0;
        for(int i=0;i<piles.length;i++){
            s = s+((piles[i]+mid-1)/mid);
            //s = s + (int) Math.ceil((double) piles[i] / mid);
        } 
        if(s>h){
           l = mid +1;
        }
        else{
         hi=mid-1;
        }
        }
       return l;
    }
}
